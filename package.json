{
  "name": "qwen-proxy",
  "displayName": "Qwen Proxy",
  "description": "OpenAI-compatible API proxy for Qwen web interface",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qwen-proxy.start",
        "title": "Qwen Proxy: Start Server"
      },
      {
        "command": "qwen-proxy.stop",
        "title": "Qwen Proxy: Stop Server"
      },
      {
        "command": "qwen-proxy.status",
        "title": "Qwen Proxy: Check Status"
      },
      {
        "command": "qwen-proxy.clearCookies",
        "title": "Qwen Proxy: Clear Cookies"
      },
      {
        "command": "qwen-proxy.openBrowser",
        "title": "Qwen Proxy: Open Browser to Login"
      }
    ],
    "configuration": {
      "title": "Qwen Proxy",
      "properties": {
        "qwen-proxy.port": {
          "type": "number",
          "default": 3001,
          "description": "Port for the proxy server"
        },
        "qwen-proxy.qwenUrl": {
          "type": "string",
          "default": "https://qwen.alibaba.com",
          "description": "Qwen web interface URL"
        },
        "qwen-proxy.autoStart": {
          "type": "boolean",
          "default": true,
          "description": "Automatically start the proxy server when the extension is activated."
        },
        "qwen-proxy.headless": {
          "type": "boolean",
          "default": true,
          "description": "Run browser in headless mode"
        },
        "qwen-proxy.enableScreenshots": {
          "type": "boolean",
          "default": false,
          "description": "Enable automatic screenshot capture for each step. Disable to save disk space or in CI environments."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "jest"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "18.x",
    "@types/supertest": "^6.0.3",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "jest": "^30.0.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "puppeteer": "^24.10.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^vscode$": "<rootDir>/test/__mocks__/vscode.js",
      "^puppeteer$": "<rootDir>/test/__mocks__/puppeteer.js"
    }
  }
}
